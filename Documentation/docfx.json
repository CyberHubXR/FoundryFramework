{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Foundry.Core.csproj",
            "Foundry.Core.Editor.csproj",
            "Foundry.Core.Serialization.csproj",
            "Foundry.Core.DesignTools.Editor.csproj",
            "Foundry.Core.DesignTools.Runtime.csproj",
            "Foundry.Core.Editor.ClassFinder.csproj",
            "Foundry.Core.Editor.UIUtils.csproj",
            "Foundry.Core.Editor.PackageManagerUtil.csproj",
            "Foundry.Core.Setup.csproj",
            "Foundry.Networking.Editor.csproj",
            "Foundry.Rendering.Editor.csproj",
            "Foundry.Core.Navigation.Editor.csproj",
            "Foundry.Player.Editor.csproj"
          ]
        }
      ],
      "dest": "Api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "files": [
          "Api/toc.yml",
          "Api/**/*.yml",
          "Api/**/*.md"
        ]
      },
      {
        "files": [
          "Manual/**/*.md",
          "Manual/**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Media/**"
        ]
      }
    ],
    "overwrite": "",
    "dest": "_site",
    "globalMetadataFiles": [
      "metadata.json"
    ],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}